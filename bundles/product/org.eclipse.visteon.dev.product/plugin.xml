<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="visteon"
         name="Visteon Studio for Coma"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="*"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.visteon.dev.product.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="org.eclipse.visteon.dev.product.Perspective"
            id="org.eclipse.visteon.dev.product.perspective">
      </perspective>
   </extension>
   <extension
         id="devComa"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Visteon IDE for Coma">
         <property
               name="appName"
               value="Visteon IDE for Coma">
         </property>
      </product>
   </extension>

</plugin>
