<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="app"
         point="org.eclipse.core.runtime.applications">
         <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.visteon.generator.headless.RunGeneration">
         </run>
      </application>
   </extension>
   <extension
         id="headless"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.visteon.generator.headless.app"
            name="HeadlessGenerator">
         <property
               name="appName"
               value="HeadlessGenerator">
         </property>
      </product>
   </extension>

</plugin>
